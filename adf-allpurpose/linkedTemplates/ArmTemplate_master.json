{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-allpurpose"
		},
		"ls_azure_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql'"
		},
		"ls_oracle_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_oracle'"
		},
		"ls_saallpurposeadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_saallpurposeadls'"
		},
		"ls_azure_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "server0085.database.windows.net"
		},
		"ls_azure_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "practiceDB"
		},
		"ls_azure_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "OO85-admin"
		},
		"ls_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-access-python.vault.azure.net/"
		},
		"ls_saallpurposeadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saallpurposeadls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-allpurpose_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azure_sql_password": {
						"value": "[parameters('ls_azure_sql_password')]"
					},
					"ls_saallpurposeadls_accountKey": {
						"value": "[parameters('ls_saallpurposeadls_accountKey')]"
					},
					"ls_oracle_connectionString": {
						"value": "[parameters('ls_oracle_connectionString')]"
					},
					"ls_azure_sql_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_server')]"
					},
					"ls_azure_sql_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_database')]"
					},
					"ls_azure_sql_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_userName')]"
					},
					"ls_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_key_vault_properties_typeProperties_baseUrl')]"
					},
					"ls_saallpurposeadls_properties_typeProperties_url": {
						"value": "[parameters('ls_saallpurposeadls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-allpurpose_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-allpurpose_ArmTemplate_0')]"
			]
		}
	]
}